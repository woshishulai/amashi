/* pages/my/my.wxss */
page {
  background-color: #f2f2f2;
}

.conta {
  position: relative;
  background-size: 750rpx 501rpx;
  background-repeat: no-repeat;
  background-position: top center;
  background-image: url(data:image/png;base64,);
}

.mybg {
  position: absolute;
  z-index: -1;
  width: 750rpx;
  height: 501rpx;
  top: 0;
  left: 0;
}

.conta_cont {
  width: 700rpx;
  margin: auto;
}

.geren {
  display: flex;
  align-items: center;
  padding-top: 20rpx;
}

.touxiang {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  /* overflow: hidden; */
}

.touxiang image {
  width: 100%;
  height: 100%;
  display: block;
}

.geren_right {
  flex: 1;
  margin-left: 22rpx;
  max-width: 400rpx;
}

.geren_right .geri_top {
  font-size: 28rpx;
  color: #fff;
}

.geren_right .geri_bot {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  color: #fff;
  margin-top: 10rpx;
}

.geren_right .geri_bot image {
  width: 11rpx;
  height: 17rpx;
  display: block;
  margin-left: 18rpx;
}

.list_cont {
  background-color: #fff;
  padding: 25rpx 22rpx 55rpx 22rpx;
  margin-top: 55rpx;
  border-radius: 20rpx;
}

.list_cont .list {
  width: 100%;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #d7d7d7;
  height: 110rpx;
  padding: 0 35rpx;
  box-sizing: border-box;
  background: none;
  font-weight: normal;
  font-size: 30rpx;
  border-radius: 0;
}

.list_cont .list image {
  width: 34rpx;
  height: 34rpx;
  display: block;
}

.list_cont .list text {
  margin-left: 45rpx;
}

.exit {
  width: 318rpx;
  height: 70rpx;
  background-color: #e6e9ec;
  line-height: 70rpx;
  font-size: 30rpx;
  color: #626262;
  text-align: center;
  margin: auto;
  border-radius: 40rpx;
  margin-top: 140rpx;
}