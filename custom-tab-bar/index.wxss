/* custom-tab-bar/index.wxss */

._tabbar {
  width: 100%;
  height: 115rpx;
  display: flex;
  align-items: center;
  background: #fff;
  font-size: 26rpx;
  color: #999999;
  border-top: 1px solid #f2f2f2;
  box-sizing: border-box;
}

._tabbar .titem {
  text-align: center;
  width: 50%;
}

._tabbar .titem .image {
  display: block;
  margin: auto;
  width: 38rpx;
  height: 42rpx;
  margin-bottom: 8rpx;
}

._tabbar .tCdk {
  color: #4e89f1;
}

._iPhoneX {
  height: 148rpx;
}