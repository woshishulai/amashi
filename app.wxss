/* 文本溢出隐藏 */
.text-hidden {
  width: 100%;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.text-more-hidden {
  width: 100%;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  /*要显示的行数*/
  -webkit-box-orient: vertical;
}
